{
  "name": "sample-app",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/dskoda1/sample-app.svg?branch=master)](https://travis-ci.org/dskoda1/sample-app)",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "watch": "NODE_ENV=development nodemon  -w server server.js",
    "ci:test": "jest --rootDir server --runInBand --forceExit",
    "test": "NODE_ENV=local_test jest --rootDir server --watchAll --runInBand",
    "test:once": "NODE_ENV=local_test jest --rootDir server --runInBand --detectOpenHandles",
    "prettier": "prettier --config .prettierrc --write \"server/**/*.{js,jsx,scss,css}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dskoda1/sample-app.git"
  },
  "author": "dskoda1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dskoda1/sample-app/issues"
  },
  "homepage": "https://github.com/dskoda1/sample-app#readme",
  "dependencies": {
    "@babel/preset-env": "^7.3.1",
    "bcrypt": "^3.0.4",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "pg": "^7.8.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2",
    "supertest-session": "^3.3.0"
  }
}
